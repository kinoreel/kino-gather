#!groovy

node {
   stage ('Starting kino-inserts') {
       def kino_tables = ['movies2numbers']
       kino_tables.each {
            echo "Starting ${it}"
            try {
                build job: "kino-inserts", parameters: [[$class: 'StringParameterValue', name: 'TABLE_NAME', value: "${it}"],
                                                        [$class: 'StringParameterValue', name: 'REGISTRY_URL', value: 'eu.gcr.io']
                                                       ]
                } catch (err) {
                    echo "Build insert-${it} failed"
                }
        }
   }
}