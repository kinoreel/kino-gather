#!groovy

node {
   stage ('Starting kino-inserts') {
       def kino_tables = ['movies2numbers']
       kino_tables.each {
            echo "Starting ${it}"
            build job: "kino-inserts", parameters: [[$class: 'StringParameterValue', name: 'TABLE_NAME', value: 'movies2numbers']
                                                     $class: 'StringParameterValue', name: 'REGISTRY_URL', value: 'eu.gcr.io']]
        }
   }
}