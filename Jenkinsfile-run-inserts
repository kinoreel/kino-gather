#!groovy

node {
   stage ('Starting kino-inserts') {
       def kino_tables = ['movies2numbers']
       kino_tables.each {
            echo "Starting ${it}"
            def table_name = ${it}
            build job: "kino-inserts", parameters: [[$class: 'StringParameterValue', name: 'TABLE_NAME', value: table_name]]
        }
   }
}